{
  "stage": 0,
  optional: ['es7.asyncFunctions', 'runtime'],
  "plugins": ["react-transform"],
  "extra": {
    "react-transform": {
        transforms: [{
        "transform": "react-transform-hmr",
        "imports": ["react"],
        "locals": ["module"]
      }, {
        "transform": "react-transform-catch-errors",
        "imports": [ "react", "./src/views/ErrorBox" ]
      }]
    }
  }
}